{
  "presets": [
    ["@inst-app/react", {"removePropTypes": false}]
  ],
  "env": {
    "cjs": {
      "presets": ["@inst-app/esx"]
    },
    "es": {
      "presets": [
        ["@inst-app/esx", {"env": {"modules": false}}]
      ]
    },
    "node-cjs": {
      "presets": [
        [
          "@inst-app/esx",
          {
            "env": {
              "targets": {"node": "8"}
            }
          }
        ]
      ]
    },
    "node-esm": {
      "presets": [
        [
          "@inst-app/esx",
          {
            "env": {
              "modules": false,
              "targets": {"node": "8"}
            }
          }
        ]
      ]
    }
  }
}
